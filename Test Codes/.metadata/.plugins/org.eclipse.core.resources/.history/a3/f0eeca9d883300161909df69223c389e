#include "movingArray.h"

float movingArray[movingArrayFrequency];
float sum = 0;
int index = 0;

float movingArrayOut(float in) {
	sum = sum - movingArray[index%10];
	sum += in;
	movingArray[index%10] = in;
	index++;
	return sum/movingArrayFrequency;
}

